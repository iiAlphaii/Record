#include <stdio.h>
#include <stdlib.h>
#include <math.h>

struct triangle
{
	int a;
	int b;
	int c;
};

typedef struct triangle triangle;
void sort_by_area(triangle* tr, int n) 
{
	int i,j;
    double p=0.0;
    double s[n];
    double temp,temp1,temp2,temp3;   
    if(n>=1 && n<=100)
    {
        for(i=0;i<n;i++)
        {
            if(tr[i].a>=1 && tr[i].a<=70 && tr[i].b>=1 && tr[i].b<=70 && tr[i].c>=1 && tr[i].c<=70 && (tr[i].a + tr[i].b)>tr[i].c && (tr[i].a + tr[i].c)>tr[i].b && (tr[i].b + tr[i].c)>tr[i].a)
            {
                p=(tr[i].a+tr[i].b+tr[i].c)/2.0;
                s[i]=p*(p-tr[i].a)*(p-tr[i].b)*(p-tr[i].c); 
            }
        }
        for(i=0;i<n;i++)
        {
            for(j=0;j<n-i-1;j++)
            {
                if(s[j]>s[j+1])
                {
                temp=s[j];
                s[j]=s[j+1];
                s[j+1]=temp;
                temp1=tr[j].a;
                tr[j].a=tr[j+1].a;
                tr[j+1].a=temp1;
                temp2=tr[j].b;
                tr[j].b=tr[j+1].b;
                tr[j+1].b=temp2;
                temp3=tr[j].c;
                tr[j].c=tr[j+1].c;
                tr[j+1].c=temp3;
                }
            }
        }
    }
   return tr;
}

int main()
{
	int n;
	scanf("%d", &n);
	triangle *tr = malloc(n * sizeof(triangle));
	for (int i = 0; i < n; i++) {
		scanf("%d%d%d", &tr[i].a, &tr[i].b, &tr[i].c);
	}
	sort_by_area(tr, n);
	for (int i = 0; i < n; i++) {
		printf("%d %d %d\n", tr[i].a, tr[i].b, tr[i].c);
	}
	return 0;
}
